# AWS Credentials (get from DataExpert.io)
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here
AWS_DEFAULT_REGION=us-west-2

# S3 Bucket for Iceberg warehouse
AWS_S3_BUCKET_TABULAR=zachwilsonsorganization-522

# Schema/namespace for your tables
SCHEMA=kouver

# Airflow secrets backend (for production)
AIRFLOW__SECRETS__BACKEND=airflow.providers.amazon.aws.secrets.secrets_manager.SecretsManagerBackend
AIRFLOW__SECRETS__BACKEND_KWARGS={"connections_prefix": "airflow/connections", "variables_prefix": "airflow/variables"}

# Anthropic API (for LLM position extraction)
ANTHROPIC_API_KEY=sk-ant-your_key_here

# Snowflake (for analytics layer)
SNOWFLAKE_ACCOUNT=your_account_identifier
SNOWFLAKE_USER=your_username
SNOWFLAKE_PASSWORD=your_password
SNOWFLAKE_WAREHOUSE=your_warehouse
SNOWFLAKE_DATABASE=your_database
SNOWFLAKE_SCHEMA=your_schema
